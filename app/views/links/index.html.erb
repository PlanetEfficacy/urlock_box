<div class="container">
  <div class="col-md-6 col-md-offset-3 text-center">
    <h1>URLockbox</h1>
    <div class="well">
      <h4>Submit a new link</h4>
        <div class="form-group text-left">
          <%= label_tag :title %>
          <%= text_field_tag :title, '', id: 'create-link-title', class: "form-control" %>
        </div>
        <div class="form-group text-left">
          <%= label_tag :url %>
          <%= url_field_tag :url, '', id: 'create-link-url', class: "form-control" %>
        </div>
        <div class="form-group">
          <%= submit_tag "Submit", id: 'create-link-button', class: "btn btn-primary" %>
        </div>
    </div>
  </div>
</div>
<div class="container">
  <div class="" id="link-list">
  </div>
</div>
